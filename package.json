{
  "name": "@remember_web/ui",
  "version": "0.0.0-beta.16",
  "description": "Remember UI Components",
  "homepage": "https://dramancompany.github.io/rui/",
  "author": "Remember",
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/dramancompany/rui.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "dev": "vite",
    "build": "rm -rf dist && tsc && node scripts/build.mjs",
    "build:visualize": "yarn build --visualize",
    "pub": "yarn build && node scripts/prepublish.mjs && cd dist && npm publish --tag latest",
    "pub:beta": "yarn build && node scripts/prepublish.mjs && cd dist && npm publish --tag beta",
    "lint": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "prettier": "prettier -c src",
    "prettier-fix": "prettier --write src",
    "storybook-nocache": "start-storybook --no-manager-cache -p 6006",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o docs"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/addons": "^6.5.12",
    "@storybook/builder-vite": "^0.2.2",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "^0.0.13",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "@vitejs/plugin-react": "^2.0.1",
    "eslint": "^8.23.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.7",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.4",
    "open": "^8.4.0",
    "prettier": "^2.7.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "rollup-plugin-visualizer": "^5.8.1",
    "styled-components": "^5.3.5",
    "typescript": "^4.6.4",
    "vite": "^3.0.7",
    "vite-plugin-dts": "^1.4.1",
    "vite-tsconfig-paths": "^3.5.0",
    "vite-plugin-svgr": "^2.2.2"
  },
  "peerDependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "styled-components": "^5.3.5"
  },
  "bugs": {
    "url": "https://github.com/dramancompany/rui/issues"
  }
}